        EXTENT_SIZE,
        INITIAL_SIZE,
        MAXIMUM_SIZE,
        AUTOEXTEND_SIZE,
        VERSION,     // To be used by NDB
        ROW_FORMAT,  // To be used by NDB
        DATA_FREE,
        STATUS
    
